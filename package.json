{
  "name": "@stackend/react",
  "version": "0.3.1",
  "description": "React components for Stackend",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-react"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint --ext ts --ext tsx --ignore-pattern \"*.style.ts\" --ignore-pattern \"*.d.ts\" .",
    "lint:fix": "eslint --fix --ext ts --ext tsx --ignore-pattern \"*.style.ts\" --ignore-pattern \"*.d.ts\" .",
    "clean": "rimraf {abuse,cms,i18n,like,pagination,reducers,shop,style-common,throbber,ui,util}/*.{js,js.map,d.ts,d.ts.map} modal/?odal*.{js,js.map,d.ts,d.ts.map}",
    "format:check": "prettier --list-different \"**/*.{ts,tsx}\" \"**/*.md\" \"**/i18n/*.json\"",
    "format": "prettier --config .prettierrc.json \"**/*.{ts,tsx}\" \"**/*.md\" \"**/i18n/*.json\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check",
    "preversion": "npm run lint",
    "extract-messages": "formatjs extract **/*.tsx"
  },
  "peerDependencies": {
    "styled-components": "5.x",
    "react": " >= 16.14 < 17",
    "react-dom": " >= 16.14 < 17",
    "react-intl": "5.x",
    "react-router": "3.x",
    "redux": "4.x",
    "react-redux": "7.x",
    "redux-thunk": "2.x",
    "@stackend/api": "1.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "es6-promise": "^3.3.1",
    "immutability-helper": "^2.9.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.20",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.11.2",
    "winston": "^3.3.3",
    "react-slick": "^0.27.13"
  },
  "devDependencies": {
    "@stackend/api": "1.0.69",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^5.10.6",
    "react-router": "^3.2.6",
    "redux": "^4.0.5",
    "react-redux": "^7.2.2",
    "redux-thunk": "^2.3.0",
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@formatjs/cli": "^2.13.15",
    "@types/config": "^0.0.36",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.8",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router": "^3.0.24",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.2"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "types": "./",
  "publishConfig": {
    "access": "public"
  }
}
