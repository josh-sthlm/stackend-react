{
  "name": "@stackend/react",
  "version": "0.5.2",
  "description": "React components for Stackend",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-react"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "babel --config-file ./.babelrc.js --source-root . --out-dir .  --relative  --source-maps --no-comments --ignore 'node_modules,**/*.d.ts,**/*.d.tsx' --extensions '.tsx,.ts'  '**/*.ts' '**/*.tsx'",
    "build-types": "tsc",
    "test": "jest",
    "lint": "eslint --ext ts --ext tsx --ignore-pattern \"*.style.ts\" --ignore-pattern \"*.d.ts\" .",
    "lint:fix": "eslint --fix --ext ts --ext tsx --ignore-pattern \"*.style.ts\" --ignore-pattern \"*.d.ts\" .",
    "clean": "del-cli {abuse,cms,i18n,like,pagination,reducers,shop,style-common,throbber,ui,util}/**/*.{js,js.map,d.ts,d.ts.map} modal/?odal*.{js,js.map,d.ts,d.ts.map}",
    "format:check": "prettier --list-different \"**/*.{ts,tsx}\" \"**/*.md\" \"**/i18n/*.json\"",
    "format": "prettier --config .prettierrc.json \"**/*.{ts,tsx}\" \"**/*.md\" \"**/i18n/*.json\" --write",
    "prepack": "npm run clean && npm run build-types && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check",
    "preversion": "npm run lint",
    "extract-messages": "formatjs extract **/*.tsx",
    "start": "webpack serve --mode development --open --hot"
  },
  "peerDependencies": {
    "@stackend/api": ">= 1.2.1 < 1.3",
    "react": " >= 16.14 < 17",
    "react-dom": " >= 16.14 < 17",
    "react-intl": "5.x",
    "react-redux": "7.x",
    "react-router": "4.x",
    "redux": "4.x",
    "redux-thunk": "2.x",
    "styled-components": "5.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "es6-promise": "^4.2.8",
    "immutability-helper": "^3.1.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.13.1",
    "react-slick": "^0.27.14",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-external-helpers": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@formatjs/cli": "^2.13.15",
    "@hot-loader/react-dom": "^16.14.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^0.1.1",
    "@stackend/api": "1.2.1",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^12.19.12",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.10",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "^5.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
