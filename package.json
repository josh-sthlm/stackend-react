{
  "name": "@stackend/react",
  "version": "1.0.1",
  "description": "React components for Stackend",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-react"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint --ext ts --ext tsx --ignore-pattern \"*.style.ts\" --ignore-pattern \"*.d.ts\" .",
    "clean": "rimraf {abuse,cms,like,pagination,reducers,shop,style-common,throbber,util}/*.{js,js.map,d.ts,d.ts.map} modal/?odal*.{js,js.map,d.ts,d.ts.map}",
    "format:check": "prettier --list-different \"**/*.{ts,tsx}\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json \"**/*.{ts,tsx}\" \"**/*.md\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check",
    "preversion": "npm run lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@stackend/api": "1.0.9",
    "es6-promise": "^3.3.1",
    "immutability-helper": "^2.9.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.20",
    "moment": "^2.22.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-modal": "^3.11.2",
    "react-router": "^3.2.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "react-redux": "^5.1.2",
    "styled-components": "^5.2.0",
    "winston": "^3.3.3",
    "react-intl": "^5.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/config": "^0.0.36",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.29",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^3.0.24",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.4.2",
    "jest-styled-components": "7.0.3",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "typings-tester": "^0.3.2"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "types": "./",
  "publishConfig": {
    "access": "public"
  }
}
