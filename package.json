    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^5.10.6",
    "react-redux": "^7.2.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "source-map-loader": "^2.0.0",
